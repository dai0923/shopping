r\n            <button\r\n              @click=\"handChangePage(2)\"\r\n              :class=\"[{ active: pageIdx === 2 }, 'last']\"\r\n            >\r\n              Articles (3)\r\n            </button>\r\n          </div>\r\n\r\n          <div class=\"tab_con Introduce_text\" v-if=\"pageIdx === 0\">\r\n            <IntroduceTab />\r\n          </div>\r\n          <div class=\"tab_con Activity_text\" v-if=\"pageIdx === 1\">\r\n            <ArticlesTab />\r\n          </div>\r\n          <div class=\"tab_con Articles_text\" v-if=\"pageIdx === 2\">\r\n            <ActivityTab />\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <div class=\"data\">\r\n        <div class=\"dashboard\">\r\n          <p>your dashboard</p>\r\n          <hr />\r\n          <div class=\"data_box\">\r\n            <p class=\"num\">367</p>\r\n            <p class=\"text\">views today</p>\r\n          </div>\r\n          <div class=\"data_box\">\r\n            <p class=\"num\">15</p>\r\n            <p class=\"text\">posts views</p>\r\n          </div>\r\n          <div class=\"data_box\">\r\n            <p class=\"num\">9</p>\r\n            <p class=\"text\">search appereances</p>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<style lang=\"scss\" scoped>\r\n.content {\r\n  max-width: 1180px;\r\n  margin: auto;\r\n  padding: 40px;\r\n  display: flex;\r\n  @media screen and (max-width: 1200px) {\r\n    display: block;\r\n  }\r\n}\r\n\r\n.profile {\r\n  width: 850px;\r\n  @media screen and (max-width: 1200px) {\r\n    width: 100%;\r\n  }\r\n}\r\n\r\n.introduce {\r\n  background-color: #fff;\r\n  position: relative;\r\n  color: #181818;\r\n  .bg {\r\n    @media screen and (max-width: 1200px) {\r\n      width: 100%;\r\n    }\r\n  }\r\n  .photo {\r\n    position: absolute;\r\n    left: 25px;\r\n    bottom: 30px;\r\n    max-width: 170px;\r\n    @media screen and (max-width: 991px) {\r\n      bottom: 0px;\r\n      top: 0px;\r\n      max-width: 20%;\r\n    }\r\n  }\r\n  .in_text {\r\n    padding: 25px 45px 35px 220px;\r\n    font-size: 14px;\r\n    @media screen and (max-width: 991px) {\r\n      padding: 25px;\r\n    }\r\n    p {\r\n      margin: 10px auto;\r\n    }\r\n    .name {\r\n      font-size: 18px;\r\n      font-weight: bold;\r\n    }\r\n  }\r\n  button {\r\n    width: 170px;\r\n    line-height: 32px;\r\n    text-align: center;\r\n    font-size: 12px;\r\n    font-weight: bold;\r\n    margin-right: 15px;\r\n    border-radius: 4px;\r\n    background-color: #0077b5;\r\n    color: #fff;\r\n    border: 1px solid transparent;\r\n    &:hover {\r\n      background-color: #fff;\r\n      color: #0275b1;\r\n      border: 1px solid #0275b1;\r\n    }\r\n    @media screen and (max-width: 991px) {\r\n      width: 100%;\r\n      margin-bottom: 10px;\r\n    }\r\n  }\r\n}\r\n\r\n.data {\r\n  width: 290px;\r\n  background-color: #fff;\r\n  padding: 25px 30px 30px;\r\n  margin-left: 40px;\r\n  @media screen and (max-width: 1200px) {\r\n    margin-left: 0px;\r\n    margin-top: 20px;\r\n    max-width: 100%;\r\n    display: block;\r\n    width: auto;\r\n  }\r\n}\r\n\r\n.data_box {\r\n  margin: 20px auto;\r\n  p {\r\n    margin: 0px;\r\n  }\r\n  .num {\r\n    color: #0275b1;\r\n    font-size: 52px;\r\n    font-weight: bold;\r\n  }\r\n  .text {\r\n    font-size: 14px;\r\n    color: #181818;\r\n  }\r\n}\r\n\r\n.tab_box {\r\n  margin-top: 20px;\r\n}\r\n.tab_btn {\r\n  border-bottom: 1px solid #e7e7e7;\r\n  margin-bottom: 30px;\r\n  font-weight: bold;\r\n  font-size: 12px;\r\n  button {\r\n    border: 1px solid #e7e7e7;\r\n    color: #181818;\r\n    width: 240px;\r\n    text-align: center;\r\n    padding: 15px 0px;\r\n    background-color: #fff;\r\n    border-bottom: 0px;\r\n    vertical-align: bottom;\r\n    &.first {\r\n      border-radius: 5px 0 0 0;\r\n    }\r\n    &.last {\r\n      border-radius: 0 5px 0 0;\r\n    }\r\n    &.active {\r\n      background-color: rgba(0, 119, 181, 1);\r\n      color: #fff;\r\n      padding-top: 22px;\r\n      border-radius: 5px 5px 0 0;\r\n    }\r\n    @media screen and (max-width: 991px) {\r\n      width: 33.3%;\r\n    }\r\n  }\r\n}\r\n\r\n.tab_con {\r\n  background-color: #fff;\r\n  padding: 30px;\r\n}\r\n</style>\r\n"],"sourceRoot":""}]}